{
    "name": "unspokenn/oanda",
    "type": "library",
    "description": "Oanda PHP Client Library.",
    "keywords": [
        "oanda",
        "laravel",
        "forex",
        "client",
        "http",
        "api"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/unspokenn/oanda/issues",
        "source": "https://github.com/unspokenn/oanda"
    },
    "authors": [
        {
            "name": "Andrew Robinson",
            "homepage": "https://github.com/thecodemill",
            "role": "Creator"
        },
        {
            "name": "Sadık Ergüven",
            "homepage": "https://github.com/unspokenn",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Unspokenn\\Oanda\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unspokenn\\Oanda\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
